{
  "name": "@fafnirical/developing.ninja",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "format": "npm run typegen && concurrently --names \"format:\" \"npm:format:*(!prettier)\" && npm run format:prettier",
    "format:eslint": "eslint --cache --cache-location node_modules/.cache/eslint --fix .",
    "format:package-json": "sort-package-json",
    "format:prettier": "prettier --cache --cache-location node_modules/.cache/prettier --ignore-path .gitignore --write .",
    "lint": "npm run typegen && concurrently --names \"lint:\" \"npm:lint:*\"",
    "lint:eslint": "eslint --cache --cache-location node_modules/.cache/eslint .",
    "lint:package-json": "sort-package-json --check",
    "lint:prettier": "prettier --cache --cache-location node_modules/.cache/prettier --ignore-path .gitignore --check .",
    "lint:typescript": "tsc --noEmit",
    "prepare": "husky",
    "start": "react-router-serve ./build/server/index.js",
    "typegen": "react-router typegen"
  },
  "dependencies": {
    "@react-router/node": "^7.10.1",
    "@react-router/serve": "^7.10.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "isbot": "^5.1.32",
    "lucide-react": "^0.555.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint-react/eslint-plugin": "^2.3.9",
    "@eslint/js": "^9.39.1",
    "@react-router/dev": "^7.10.1",
    "@tailwindcss/vite": "^4.1.17",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sort-package-json": "^3.5.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": "^24.11.0"
  }
}
