{
  "extends": [
    "@tsconfig/strictest/tsconfig.json",
    "@tsconfig/node18/tsconfig.json"
  ],
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "**/.*.js",
    "**/*.js",
    "**/.*.cjs",
    "**/*.cjs",
    "**/.*.mjs",
    "**/*.mjs",
    "**/*.jsx",
    "**/.*.ts",
    "**/*.ts",
    "**/.*.cts",
    "**/*.cts",
    "**/.*.mts",
    "**/*.mts",
    "**/*.tsx"
  ]
}
